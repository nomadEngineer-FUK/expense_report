{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
      "#app": [
        "./node_modules/nuxt/dist/app"
      ],
      "#head": [
        "./.nuxt/types/head"
      ],
      "#imports": [
        "./.nuxt/types/imports"
      ]
    },
    "strict": true,
    "incremental": true
  }
}
