{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
      "#app": [
        "./node_modules/nuxt/dist/app"
      ],
      "#head": [
        "./.nuxt/types/head"
      ],
      "#imports": [
        "./.nuxt/types/imports"
      ],
      "~/types/*": ["types/*"],
      "@/types/*": ["types/*"]
    },
    "strict": true,
    "incremental": true,
    "outDir": "dist",
  },
  "include": [
    "**/*.ts",
    "**/*.d.ts",
    "**/*.tsx",
    "**/*.vue",
    ".nuxt/types/",
    ".eslintrc.cjs"
  ]
}
